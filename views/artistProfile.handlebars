{{!-- Javascript that is collecting the tattoo artists id --}}
<script>
    const userId = "{{ id }}"
</script>
{{!-- Container that holds the tattoo artists information --}}
<div class="container">
    <div class="row">
        <div class="col s12 m6 l7">
            <div class="tattooArtistDetails">
                <h2>{{ firstName }} {{ lastName }}</h2>
                <p>{{ location }} {{ city }} {{ state }} {{ zip }}</p>
                <p>{{ specialization }} </p>
                <p>{{ pricing }} </p>
            </div>
        </div>
    </div>
    {{!-- Photos Container that will hold all of the photos posted by the tattoo artist --}}
    <div class="photosContainer">
        {{#if photos}}
        {{#each photos}}
        <img src="{{ this }}" alt="Tattoo Image" data-target="{{ @index }}" class="tattooArtistsUploadedImages modal-trigger">
        {{!-- tattoo image modal that displays the tattoo details on click --}}
        <div class="modal" id="{{ @index }}">
            <div class="modal-content">
                <p>TESTING TO SEE IF THIS IS A UNIQUE MODAL {{ @index }}</p>
                <img class="tattooImageInModal" src="{{ this }}" alt="">
            </div>
        </div>
        {{/each}}
        {{else}}
        {{!-- This sentence displays if the tattoo artist hasn"t posted any photos --}}
        <p>Text if the images don"t render</p>
        {{/if}}
    </div>
    {{!-- Add Photo Modal --}}
    <div class="modal" id="addPhotoModal">
        <div class="modal-content">
            <form method="post" enctype="multipart/form-data">
                <input id="file" class="uploadedPhoto" type="url" accept="image/*">
                <div class="row">
                    <div class="col s12">
                        <div class="row">
                            <div class="input-field col s12">
                                <input type="text" id="autocomplete-input" class="autocomplete">
                                <label class="description" for="autocomplete-input">Description</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="input-field col s12 m6">
                    <select class="tattooStyle" multiple>
                        <option value="Abstract">Abstract</option>
                        <option value="Ambigram">Ambigram</option>
                        <option value="American Traditional">American Traditional</option>
                        <option value="Anatomical">Anatomical</option>
                        <option value="Biomechanical">Biomechanical</option>
                        <option value="Black and Grey">Black and Grey</option>
                        <option value="Blackout">Blackout</option>
                        <option value="Blackwork">Blackwork</option>
                        <option value="Blast Over">Blast Over</option>
                        <option value="Broken Glass">Broken Glass</option>
                        <option value="Paint Brush Stroke">Paint Brush Stroke</option>
                        <option value="Celtic">Celtic</option>
                        <option value="Chicano">Chicano</option>
                        <option value="Dotwork">Dotwork</option>
                        <option value="Geometric">Geometric</option>
                        <option value="Glitch">Glitch</option>
                        <option value="Glow in the Dark">Glow in the Dark</option>
                        <option value="UV Ink">UV Ink</option>
                        <option value="Gradient">Gradient</option>
                        <option value="Graffiti">Graffiti</option>
                        <option value="Hyper Realistic">Hyper Realistic</option>
                        <option value="Inverted">Inverted</option>
